APPLY_USERSPACE			= yes	# Apply the userspace keymap (only usable in userspace rules.mk)

# all boards use these
COMBO_ENABLE 			= yes 	# Enables use of combos
MOUSEKEY_ENABLE 		= yes	# Enables mouse keys

# this board uses these
ENCODER_ENABLE			= yes	# Enables use of one or more encoders
CAPS_WORD_ENABLE		= yes	# Enables shift-based caps word

CONSOLE_ENABLE 			= no

# keyboard does not have OLED or RGB
OLED_ENABLE 			= no
RGBLIGHT_ENABLE 		= no    # Enable keyboard RGB underglow
